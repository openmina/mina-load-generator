{
  "name": "mina-load-generator",
  "version": "0.1.0",
  "description": "Generates varioius kinds of ZkApp transactions",
  "author": "alexander.koptelov@viablesystems.io",
  "license": "Apache-2.0",
  "keywords": [
    "mina-zkapp",
    "mina-zk-app",
    "mina-dapp",
    "zkapp"
  ],
  "type": "module",
  "main": "build/src/main.js",
  "types": "build/src/main.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "buildw": "tsc -p tsconfig.json --watch",
    "coverage": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --coverage",
    "format": "prettier --write --ignore-unknown **/*",
    "prepare": "husky install",
    "test": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js",
    "testw": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --watch",
    "lint": "npx eslint src/* --fix",
    "main": "node build/src/main.js"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint src/* --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.7.0",
    "eslint-plugin-snarkyjs": "^0.1.0",
    "husky": "^7.0.1",
    "jest": "^29.5.0",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "snarkyjs": "github:openmina/snarkyjs#openmina-ci"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^10.0.2",
    "@types/express": "^4.17.17",
    "commander": "^10.0.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.0",
    "tslog": "^4.7.5"
  }
}
